public class App {
    public static void main(String[] args) {
        String word = " Hello";
        Stack stack = new Stack(word.length());
        char[] wordarr = word.toCharArray();

        for(int i = 0; i < wordarr.length; i++){
            stack.push(wordarr[i]);
        }

        while(!stack.isEmpty()){
            System.out.println((char) stack.pop());
        }

    }
}
